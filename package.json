    "animate.css": "^4.1.1",
    "ant-design-vue": "^4.2.6",
    "axios": "^1.13.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-vue": "^5.3.0",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.3",
    "tailwindcss": "^4.1.16",
    "vue": "^3.5.22",
    "vue-i18next": "^0.9.4",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@iconify/vue": "^5.0.0",
    "@playwright/test": "^1.56.1",
    "@prettier/plugin-oxc": "^0.0.4",
